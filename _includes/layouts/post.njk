{% endif %}



  <div class="sidebarwrapper2">
{% set postslist = collections['Featured'] %}
  {% include "components/postslist-sidebar.njk" %}
  </div>
  
 
  
  </aside>
</div>
</div>


  {% if tags %}
    <div class="NewsSummaryPrice detail"><h4>Tags</h4>
    {% for tag in tags %}
      {%- if tag != "post" -%}
        {% set tagUrl %}/treatments/{{ tag }}/{% endset %}
       <div class="tag"><a href="{{ tagUrl | url }}" rel="tag">{{ tag }}</a></div>
      {%- endif -%}
    {% endfor %}
    </div>
  {% endif %}